<?xml version="1.0" encoding="UTF-8"?>
<project name="config.deds">
	<echo>[deds.xml] Declare module vars (config.deds)</echo>

	<!-- project variables (workaround to use with CDATA - PROPERTY NAME CAN'T REPEAT IN OTHERS PROJECT FILES) -->
	<property name="prop.cmd.footer"><![CDATA[
	<script type="text/javascript">
		alert(123);
	</script>
	]]></property>
	
	<!-- variables (evaluating properties) -->
	<property name="cmd.footer" value="" />
	<var name="cmd.footer" value="${cmd.footer} ${prop.cmd.footer}" />
	
</project>